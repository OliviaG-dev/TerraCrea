    "@react-navigation/native-stack": "^7.3.21",
    "@react-navigation/stack": "^7.4.7",
    "@supabase/supabase-js": "^2.52.1",
    "expo": "~53.0.17",
    "expo-image-picker": "^16.1.4",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
