  "main": "index.ts",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "check-node": "node -v && npm -v",
    "preinstall": "node -e \"const v=process.versions.node; if(parseInt(v.split('.')[0])<22) throw new Error('Node.js version 22 or higher is required')\"",
