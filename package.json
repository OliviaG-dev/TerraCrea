{
  "name": "terracrea",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "npx expo export:web",
    "check-node": "node -v && npm -v",
    "preinstall": "node -e \"const v=process.versions.node; if(parseInt(v.split('.')[0])<22) throw new Error('Node.js version 22 or higher is required')\"",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:debug": "vitest --reporter=verbose"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@supabase/supabase-js": "^2.52.1",
    "dotenv": "^17.2.1",
    "expo": "~51.0.28",
    "expo-image-picker": "~15.1.0",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "typescript": "~5.3.3",
    "react-native-web": "~0.19.10",
    "@expo/webpack-config": "^19.0.1",
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "babel-preset-expo": "~11.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^14.3.1",
    "@types/react": "~18.2.0",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "vitest": "^3.2.4",
    "webpack": "^5.76.0"
  },
  "private": true
}
